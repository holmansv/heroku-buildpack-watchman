#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir> <env-dir>

set -eu

VENDOR_BIN_DIR="$HOME/vendor/bin"
WATCHMAN="./watchman"
STATE_DIR="$HOME/watchman_state"

echo "******************************************************"
ls
echo "******************************************************"

mkdir -p "$STATE_DIR"
mkdir -p "$VENDOR_BIN_DIR"

mv "$WATCHMAN" "$VENDOR_BIN_DIR"

echo "******************************************************"
ls "$VENDOR_BIN_DIR"
echo "******************************************************"
